   "source": [
    "def computeCost(x, y, theta):\n",
    "    m = len(y)\n",
    "    return (1/(2.0*m))*np.sum(np.square(np.matmul(x, theta)-y))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
