  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def gradDescent(x, y, theta, alpha, iters):\n",
    "    m = len(y)\n",
    "    J_history = np.zeros((iters, 1))\n",
    "    theta = np.zeros(x.shape[1])\n",
    "    \n",
    "    for i in range(iters):\n",
    "        #diff = np.dot(x, theta)- y\n",
    "        #prod = np.dot(np.transpose(x), diff)\n",
    "        prod = np.matmul(x.T, np.matmul(x, theta)-y)\n",
    "        theta = theta - (alpha/m)*prod\n",
    "        \n",
    "        J_history[i] = computeCost(x, y, theta)\n",
    "        \n",
    "    return theta, J_history "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
